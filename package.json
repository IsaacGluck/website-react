{
  "name": "website-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://IsaacGluck.com",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "email-validator": "^2.0.4",
    "npm-run-all": "^4.1.3",
    "particles.js": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hamburger-menu": "^1.1.1",
    "react-lazy-load": "^3.0.13",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.7.10",
    "react-toastify": "^5.3.2",
    "react-vertical-timeline-component": "^2.1.3",
    "sanitize.css": "^11.0.0"
  },
  "scripts": {
    "build-css": "sass src/:src/ --load-path=./src --load-path=./node_modules --no-source-map",
    "watch-css": "sass src/:src/ --load-path=./src --load-path=./node_modules --no-source-map --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "deploy": "yarn build; now --prod",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "sass": "^1.91.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
